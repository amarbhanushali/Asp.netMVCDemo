<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CashBack Systems - @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
<div class="header">
    <div class="row">
        <div class="col-6 col-md-4 col-lg-3 logo">
            <img src="@Url.Content("~/Content/images/logo.jpg")" alt="Ahmed Pro Partner" />
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12 col-md-12 col-lg-12" style="padding-right: 70px;">
        @Html.Partial("_LoginPartial")
    </div>
</div>
   
        @RenderBody()
        <div id="dropDownSelect1"></div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
